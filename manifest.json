{
    "manifest_version": 3,
    "name": "Langlearner",
    "version": "1.0",
    "description": "Learn one new word daily in your target language.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "options_page": "dashboard.html",

    "permissions": ["storage"],
    "icons": {
      "128": "icon.png"
    }
  }
  